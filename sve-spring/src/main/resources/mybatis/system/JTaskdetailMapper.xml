    <sql id="selectJTaskdetailVo">
        select ztaskdid, ztaskhid, zproctype, zprocuserid, zhandleduserid, zisread, zfeedback, zprocdate, zisdeleted, zdatecreated, zcreatedby, zdateupdated, zupdatedby, zprocusertype from j_taskdetail
