    </delete>

    <delete id="deleteJTaskdetailByIds" parameterType="String">
        delete from j_taskdetail where ztaskdid in 
        <foreach item="ztaskdid" collection="array" open="(" separator="," close=")">
            #{ztaskdid}
        </foreach>
    </delete>
    
